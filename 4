algoritmo "PreenchimentoVetorMultiplos3"
var
   vetor: vetor[1..20] de inteiro
   i, valor: inteiro
inicio
   i <- 1
   valor <- 3
   
   enquanto i <= 20 faca {
      vetor[i] <- valor
      valor <- valor + 3
      i <- i + 1
   }
   
   // Exibindo o vetor preenchido
   i <- 1
   enquanto i <= 20 faca {
      escreval("Posição ", i, ": ", vetor[i])
      i <- i + 1
   }
fimalgoritmo
